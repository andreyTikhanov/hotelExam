<Page x:Class="MyHotel.View.SettingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MyHotel.View"
      mc:Ignorable="d" 
      d:DesignHeight="550" d:DesignWidth="800"
      Title="SettingPage">

    <Grid Background="Bisque">
        <StackPanel>
            <Button Click="btnChangeUserClick" Margin="0 100 0 20" Style="{StaticResource myButton}" Content="Изменить профиль"></Button>
            <Button Margin="0 20 0 20" Style="{StaticResource myButton}" Content="Управление картой"/>
            <Button Style="{StaticResource myButton}" Content="Назад" Margin="0 60 0 0" Click="btnBack_Click"/>

        </StackPanel>
    </Grid>
</Page>
